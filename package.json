{
  "name": "@msafe/sui-app-store",
  "version": "0.0.222",
  "description": "MSafe Sui app store repository",
  "author": "Momentum Safe",
  "license": "MIT",
  "type": "commonjs",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "package.json"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "yarn clean && tsup --platform browser --format cjs,esm --minify --dts",
    "build-yalc": "yarn build && yalc publish --private",
    "test": "jest",
    "coverage": "jest --coverage",
    "prerelease": "yarn build && chmod +x ./scripts/prerelease.sh && ./scripts/prerelease.sh",
    "lint": "npx eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "npx eslint . --ext .js,.ts,.jsx,.tsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Momentum-Safe/msafe-sui-wallet.git"
  },
  "homepage": "https://github.com/Momentum-Safe/msafe-sui-wallet",
  "bugs": {
    "url": "https://github.com/Momentum-Safe/msafe-sui-wallet/issues"
  },
  "keywords": [
    "MSafe",
    "MSafe SDK",
    "MSafe wallet",
    "MSafe Sui"
  ],
  "dependencies": {
    "@alphafi/alphafi-sdk": "^0.0.67",
    "@alphafi/stsui-sdk": "^0.0.21",
    "@cetusprotocol/aggregator-sdk": "^0.3.32",
    "@cetusprotocol/cetus-periphery-sdk": "^3.5.0",
    "@cetusprotocol/cetus-sui-clmm-sdk": "^5.3.4",
    "@cetusprotocol/cetus-xcetus-sdk": "^0.1.1",
    "@cetusprotocol/vaults-sdk": "^0.1.15",
    "@firefly-exchange/library-sui": "^2.4.18",
    "@mysten/bcs": "^1.5.0",
    "@mysten/sui": "^1.8.0",
    "@open-rpc/client-js": "^1.8.1",
    "@pythnetwork/pyth-sui-js": "^2.1.0",
    "@suilend/frontend-sui": "^0.2.52",
    "@suilend/sdk": "^1.1.45",
    "@suilend/springsui-sdk": "^1.0.20",
    "axios": "^1.6.7",
    "bignumber.js": "^9.1.2",
    "bucket-protocol-sdk": "^0.10.17",
    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "kriya-dev-sdk": "1.0.6",
    "luxon": "^3.4.4",
    "nanoid": "3",
    "navi-sdk": "1.6.0",
    "sort-keys-recursive": "^2.1.10",
    "turbos-clmm-sdk": "^2.2.2"
  },
  "devDependencies": {
    "@msafe/sui3-utils": "^3.1.66",
    "@mysten/sui.js": "0.54.1",
    "@mysten/wallet-standard": "0.11.6",
    "@types/jest": "^29.2.1",
    "@types/luxon": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@msafe/sui3-utils": ">3.0.0",
    "@mysten/sui.js": ">0.50.0",
    "@mysten/wallet-standard": ">0.11.0"
  }
}
